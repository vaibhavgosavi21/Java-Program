import java.util.Scanner;
class Pattern24
{
	public static void main(String[] args) 
	{
		System.out.print("Enter the Number: ");
		int n = new Scanner(System.in).nextInt();
		int a = 1;
		char b = 'a';
		for(int i=1;i<=n;i++){
			for(int j=1;j<=i;j++){
				if((i)%2!=0)
					System.out.print(b+" ");
				else
					System.out.print(a+" ");
			}
			System.out.println();
		}
	}
}
